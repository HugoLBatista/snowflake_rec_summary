CREATE TABLE IF NOT EXISTS rec_session_summary (
	account_id int not null,	
	start_time timestamp not null,
	visits int null,
  first_time_customers int null,
	first_time_buyers int null,
	stealth_visits int null,
	bounced_visits int null,
	impressions int null,
	pages int null,
	total_revenue float null,
	total_quantity int null,
	total_transactions int null,
	unique_items_converted int null,	
	converted_revenue_on_recommendation float null,
	converted_visits_with_rec_clickthrough int null,
	converted_visits_without_rec_clickthrough int null,		
	converted_visits_with_purchased_clickthrough int null,
	converted_visits_without_purchased_clickthrough int null,	
	converted_visits_with_purchased_rec int null,
	converted_visits_without_purchased_rec int null,
	converted_visits_non_recommendation_with_rec_clickthrough int null,	
	converted_visits_non_clicked_recommendation int null,	
	converted_items_non_clicked_recommendation int null,
	converted_items_with_purchased_rec int null,
	converted_items_without_purchased_rec int null,
	converted_items_with_purchased_clickthrough int null,
	converted_items_without_purchased_clickthrough int null,
	converted_items_with_rec_clickthrough int null,
	converted_items_without_rec_clickthrough int null,
	converted_items_non_recommendation_without_rec_clickthrough int null,
	converted_items_recommendation_without_rec_clickthrough int null,
    converted_items_non_recommendation_with_rec_clickthrough int null,
	converted_items_on_recommendation int null,	
	nonconverted_visits_with_rec_clickthrough int null,
	nonconverted_visits_without_rec_clickthrough int null,
	converted_visits_non_recommendation_without_rec_clickthrough int null,	
	converted_visits_recommendation_without_rec_clickthrough int null,
	revenue_with_recommendation_without_rec_clickthrough float null,
	revenue_with_non_recommendation_without_rec_clickthrough float null,
	revenue_with_purchased_rec float null,
	revenue_without_purchased_rec float null,
	revenue_with_purchased_clickthrough float null,
	revenue_without_purchased_clickthrough float null,
	revenue_with_rec_clickthrough float null,
	revenue_without_rec_clickthrough float null,
	revenue_with_non_clicked_recommendation float null,
	revenue_with_non_recommendation_with_rec_clickthrough float null,
	transactions_non_clicked_recommendation int null,
	transactions_non_recommendation_with_rec_clickthrough int null,
	transactions_recommendation_without_rec_clickthrough int null,
	transactions_non_recommendation_without_rec_clickthrough int null,
	transactions_with_purchased_clickthrough int null,
	transactions_without_purchased_clickthrough int null,
	transactions_with_purchased_rec int null,
	transactions_without_purchased_rec int null,
	transactions_with_rec_clickthrough int null,
	transactions_without_rec_clickthrough int null	

	PRIMARY KEY (account_id, start_time)
	)cluster by (start_time::date, account_id);
